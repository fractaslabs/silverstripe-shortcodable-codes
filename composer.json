{
  "name": "fractas/shortcodable-codes",
  "description": "Custom shortcodes for Shortcodable module",
  "type": "silverstripe-vendormodule",
  "homepage": "http://github.com/fractaslabs/silverstripe-shortcodable-codes",
  "keywords": ["silverstripe", "shortcode", "silverstripe shortcodes"],
  "license": "BSD-3-Clause",
  "support": {
    "issues": "http://github.com/fractaslabs/silverstripe-shortcodable-codes/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "sheadawson/silverstripe-shortcodable": "dev-master",
    "silverstripe/vendor-plugin": "^1.0"
  },
  "autoload": {
      "psr-4": {
          "Fractas\\ShortcodableCodes\\": "src/Shortcodes"
      }
  },
  "extra": {
    "screenshots": [
      "docs/en/images/shortcode-placeholders.png",
      "docs/en/images/vimeo-video-id-example.png",
      "docs/en/images/youtube-video-id-example.png"
    ],
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "expose": [
      "client/dist"
    ]
  }
}
